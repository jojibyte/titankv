{
  "name": "titankv",
  "version": "2.0.0",
  "description": "ultra-fast embedded key-value engine for node.js. zstd compression, redis-like api, wal persistence. no server needed.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "install": "node-gyp-build || cmake-js compile -- -DCMAKE_POLICY_VERSION_MINIMUM=3.5",
    "rebuild": "cmake-js compile -- -DCMAKE_POLICY_VERSION_MINIMUM=3.5",
    "test": "node test/index.js",
    "benchmark": "node test/benchmark.js",
    "test:all": "node test/index.js && node test/benchmark.js"
  },
  "keywords": [
    "kv",
    "key-value",
    "database",
    "cache",
    "store",
    "redis",
    "sorted-set",
    "hash",
    "pubsub",
    "transactions",
    "leveldb",
    "native",
    "fast",
    "memory",
    "persistence",
    "wal",
    "ttl",
    "zstd",
    "compression"
  ],
  "author": "jojibyte",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jojibyte/titankv.git"
  },
  "homepage": "https://github.com/jojibyte/titankv#readme",
  "bugs": {
    "url": "https://github.com/jojibyte/titankv/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "lib",
    "include",
    "src",
    "prebuilds",
    "CMakeLists.txt",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "cmake-js": "^8.0.0",
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "binary": {
    "napi_versions": [
      8
    ]
  }
}